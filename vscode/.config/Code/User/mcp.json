{
	"servers": {
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"gallery": true
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		"deepwiki": {
			"url": "https://mcp.deepwiki.com/sse",
			"type": "http"
		},
		"github": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		},
		"markitdown": {
			"command": "uvx",
			"args": [
				"markitdown-mcp"
			]
		}
	},
	"inputs": []
}